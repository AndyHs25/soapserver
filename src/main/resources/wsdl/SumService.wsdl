        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>

  <wsdl:service name="SumService">
    <wsdl:port binding="tns:SumServiceSOAPBinding" name="SumServiceSOAPBinding">
      <soap:address location="http://localhost:8080/services/soap/SumService"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>