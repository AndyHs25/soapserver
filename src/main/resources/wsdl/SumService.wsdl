<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions
        name="SumService"
        targetNamespace="http://idat.pe/SumService/wsdl"
        xmlns:tns="http://idat.pe/SumService/wsdl"
        xmlns:ns1="http://idat.pe/SumService/schema"
        xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
        xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <wsdl:types>
    <xsd:schema targetNamespace="http://idat.pe/SumService/wsdl" >
      <xsd:import namespace="http://idat.pe/SumService/schema" schemaLocation="SumService.xsd"/>
