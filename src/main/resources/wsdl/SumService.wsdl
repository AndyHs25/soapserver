    </wsdl:operation>
    <wsdl:operation name="DeletePerson">
      <wsdl:input message="tns:DeletePersonRequest"/>
      <wsdl:output message="tns:DeletePersonResponse"/>
    </wsdl:operation>
  </wsdl:portType>

  <wsdl:binding name="SumServiceSOAPBinding" type="tns:SumServicePort">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="GetSum">
      <soap:operation soapAction="http://idat.pe/SumService/GetSum"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="GetPersona">
      <soap:operation soapAction="http://idat.pe/SumService/GetPersona"/>
